{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "metarhia",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}
